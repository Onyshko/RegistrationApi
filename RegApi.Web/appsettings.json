{
  "KeyVaultUri": "https://regapi-secrets.vault.azure.net/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ConnectionStrings": "CONNECTION_STRING"
  },
  "JWTSettings": {
    "securityKey": "JWT_SECURITY_KEY",
    "validIssuer": "VALID_ISSUER",
    "validAudience": "VALID_AUDIENCE",
    "expiryInMinutes": 5
  },
  "EmailConfiguration": {
    "From": "EMAIL",
    "SmtpServer": "SMTP_SERVER",
    "Port": 465,
    "Username": "EMAIL",
    "Password": "EMAIL_PASSWORD"
  },
  "GoogleAuthentication": {
    "ClientId": "GOOGLE_CLIENT_ID",
    "ClientSecret": "GOOGLE_CLIENT_SECRET",
    "CallbackPath": "GOOGLE_CALLBACK_PATH"
  },
  "AllowedHosts": "*"
}
